{% extends "base.html.twig" %}

{% block body %}
    <section class="main">
        <h1>Strings list</h1>
        <table>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Date Created</th>
                </tr>
            </thead>
            <tbody>
                {% for item in strings %}
                    <tr>
                        <td>{{ item.id }}</td>
                        <td>
                            <a href="{{ path('app_linked_list_edit_string', {'id': item.id}) }}">
                                {{ item.name }}
                            </a>
                        </td>
                        <td>{{ item.dateCreatedString }}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    </section>
{% endblock %}

{% block title %}

{% endblock %}

